<h1>Add Season</h1>
<form action="/seasons" method="post">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<p>
		<label>Year</label>
		<input type="text" name="season[season]">
	</p>
	<input type="submit" value="create">
</form>
<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
	<p><%= e %></p>
	<% end %>
<% end %>